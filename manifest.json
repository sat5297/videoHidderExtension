{
  "manifest_version": 3,
  "name": "Video Reader + Auto Blur",
  "version": "3.0",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_title": "Auto Blur Keyword"
  }
}
