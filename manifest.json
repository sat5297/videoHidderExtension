{
  "manifest_version": 3,
  "name": "Video Reader",
  "version": "1.1",
  "description": "Reads details of the video playing on the current webpage.",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Video Reader"
  }
}
